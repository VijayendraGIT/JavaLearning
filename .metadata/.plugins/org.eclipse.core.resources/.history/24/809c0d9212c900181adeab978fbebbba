package class9;

public class CallByValueAndReference {

	int a;
	int b;
	
	int sum(int x, int y){
		return x+y;
	}
	
	public static void main(String[] args) {

		CallByValueAndReference obj1 = new CallByValueAndReference();
		int c = obj1.sum(10, 20);
		System.out.println("Sum value is "+c);

	}

}
